dnl Process this file with autoconf to produce a configure script.
AC_INIT(src/RMComm/MW-CondorPVM/MWCondorPvmRC.h)

dnl Checks for programs.
AC_PROG_INSTALL
AC_PROG_MAKE_SET
AC_PROG_RANLIB

dnl Checks for libraries.
dnl Replace `main' with a function in -lMW:
AC_CHECK_LIB(MW, main)
dnl Replace `main' with a function in -lMWcondorpvmmaster:
AC_CHECK_LIB(MWcondorpvmmaster, main)
dnl Replace `main' with a function in -lMWcondorpvmworker:
AC_CHECK_LIB(MWcondorpvmworker, main)
dnl Replace `main' with a function in -lMWfilemaster:
AC_CHECK_LIB(MWfilemaster, main)
dnl Replace `main' with a function in -lMWfileworker:
AC_CHECK_LIB(MWfileworker, main)
dnl Replace `main' with a function in -lcondorapi:
AC_CHECK_LIB(condorapi, main)
dnl Replace `main' with a function in -lnsl:
AC_CHECK_LIB(nsl, main)
dnl Replace `main' with a function in -lpvm3:
AC_CHECK_LIB(pvm3, main)

dnl Checks for header files.
AC_HEADER_STDC
AC_CHECK_HEADERS(fcntl.h limits.h sys/time.h unistd.h)

dnl Checks for typedefs, structures, and compiler characteristics.
AC_C_CONST
AC_HEADER_TIME
AC_STRUCT_TM
AC_TYPE_UID_T

dnl Checks for library functions.
AC_FUNC_VPRINTF
AC_CHECK_FUNCS(getcwd gethostname gettimeofday mkdir strstr)

AC_OUTPUT(examples/skel/Makefile examples/fib/Makefile src/Makefile Makefile src/RMComm/MW-File/Makefile src/RMComm/MW-CondorPVM/Makefile)
